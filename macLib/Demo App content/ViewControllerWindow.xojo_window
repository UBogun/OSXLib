	#tag Event
		Sub Action(index as Integer)
		  select case index
		  case 0
		    Options = NSViewControllerTransitionOptions.CrossfadeTransition
		  case 1
		    Options = NSViewControllerTransitionOptions.LeftSlideTransition
		  case 2
		    Options = NSViewControllerTransitionOptions.RightSlideTransition
		  case 3
		    Options = NSViewControllerTransitionOptions.UpSlideTransition
		  case 4
		    Options = NSViewControllerTransitionOptions.DownSlideTransition
		  end select
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events OSXLibEnhancedPushbutton1
	#tag Event
		Sub Action()
		  dim subc as AppleViewController = AppleViewController.MakefromPtr (MainController.ChildViewControllers.PtrAtIndex(0))
		  dim subc1 as AppleViewController = AppleViewController.MakefromPtr (MainController.ChildViewControllers.PtrAtIndex(1))
		  
		  dim view as AppleView = subc.View
		  if view.SuperView <> nil then
		    MainController.Transition subc, subc1, Options
		  Else
		    MainController.Transition subc1, subc, Options
		  end if
		  self.Invalidate
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events OSXLibCanvas2
	#tag Event
		Sub Paint(g as applecgcontext, Rect as FoundationFrameWork.NSRect)
		  g.DrawLinearGradient new AppleCGGradient (&c883BE055, &c0F388E88, &cD2FFC400), rect.Origin, _
		  FoundationFrameWork.NSMakePoint(rect.Size_.width, rect.Size_.height)
		  g.AddEllipse rect
		  g.clip
		  g.DrawRadialGradient new AppleCGGradient (&cFF757700, &cD2FFC400, &c883BE000, &c0F388E00), rect.center, 2,  _
		  rect.center, rect.size_.height
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub Shown()
		  me.AppleObject.WantsLayer = True
		End Sub
	#tag EndEvent
