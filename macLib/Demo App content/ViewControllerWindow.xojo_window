#tag WindowCode
	#tag Event
		Sub Open()
		  MainController = new AppleViewController
		  dim mwindow as new AppleWindow(self)
		  MainController.view = mwindow.ContentView
		  mwindow.ContentView.WantsLayer = true
		  
		  dim subc as new AppleViewController
		  subc.view = OSXLibCanvas1.AppleObject
		  MainController.AddChildViewController subc
		  
		  dim subc1 as new AppleViewController
		  subc1.view = OSXLibCanvas2.AppleObject
		  MainController.AddChildViewController subc1
		  
		  Options = NSViewControllerTransitionOptions.CrossfadeTransition
		  
		  
		  self.Invalidate
		  
		End Sub
	#tag EndEvent


	#tag Property, Flags = &h21
		Private MainController As AppleViewController
	#tag EndProperty

	#tag Property, Flags = &h21
		Private Options As NSViewControllerTransitionOptions
	#tag EndProperty


#tag EndWindowCode

#tag Events OSXLibCanvas1
	#tag Event
		Sub Paint(g as applecgcontext, Rect as FoundationFrameWork.NSRect)
		  g.DrawLinearGradient new AppleCGGradient (&cFF757700, &cD2FFC400, &c883BE000, &c0F388E00), rect.Origin, _
		  FoundationFrameWork.NSMakePoint(rect.Size_.width, rect.Size_.height)
		End Sub
	#tag EndEvent
	#tag Event
		Sub Shown()
		  me.AppleObject.WantsLayer = True
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events OSXLibEnhancedPushbutton2
	#tag Event
		Sub Action()
		  dim view as  AppleView = TextArea1.AppleObject
		  static controller as AppleViewController
		  if controller = nil then
		    controller = new AppleViewController
		    Controller.view = view
		  end if
		  try
		    MainController.PresentAsPopover controller, me.AppleObject.Bounds, me.AppleObject, FoundationFrameWork.NSRectEdge.maxx, _
		    ApplePopover.NSPopoverBehavior.Semitransient
		  catch
		    MainController.Dismiss controller
		  end try
		End Sub
	#tag EndEvent
#tag EndEvents
