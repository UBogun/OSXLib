#tag WindowCode
	#tag Method, Flags = &h1000
		Sub Constructor(aProtocol as AppleLibProtocol)
		  Protocol = aProtocol
		  me.Title = "Protocol Introspection for "+Protocol.Name
		  Super.Constructor
		  
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h0
		Protocol As AppleLibProtocol
	#tag EndProperty


#tag EndWindowCode

#tag Events PropertyBox
	#tag Event
		Sub Open()
		  dim myprops() as AppleLibProperty = Protocol.Properties
		  for q as uinteger = 0 to myprops.Ubound
		    me.AddRow  myprops(q).Name, myprops(q).AttributeNames.toCommaList, myprops(q).AttributeValuesTranslated.tocommalist
		  next
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events MethodBox
	#tag Event
		Sub Open()
		  
		  dim mymethods() as AppleLibMethodDescription = Protocol.ClassMethods
		  for q as uinteger = 0 to mymethods.Ubound
		    me.AddRow  mymethods(q).SelectorName, mymethods(q).Types, "", ""
		  next
		  mymethods = Protocol.RequiredClassMethods
		  for q as uinteger = 0 to mymethods.Ubound
		    me.AddRow  mymethods(q).SelectorName, mymethods(q).Types, "x", ""
		  next
		  mymethods = Protocol.InstanceMethods
		  for q as uinteger = 0 to mymethods.Ubound
		    me.AddRow mymethods(q).SelectorName, mymethods(q).Types, "", "x"
		  next
		  mymethods = Protocol.RequiredInstanceMethods
		  for q as uinteger = 0 to mymethods.Ubound
		    me.AddRow  mymethods(q).SelectorName, mymethods(q).Types, "x", "x"
		  next
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events ConformsBox
	#tag Event
		Sub Open()
		  dim conformsto() as AppleLibProtocol = Protocol.ConformsToProtocols
		  for q as uinteger = 0 to conformsto.Ubound
		    me.AddRow conformsto(q).Name
		  next
		End Sub
	#tag EndEvent
#tag EndEvents
