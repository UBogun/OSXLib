#tag Events PushButton1
	#tag Event
		Sub Open()
		  dim apb as  AppleButton = me.AppleObject
		  apb.ImageHugsTitle = true
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub Action()
		  TextArea1.AppendText "Accelerator level (Double Value): "+ me.AppleObject.DoubleValue.ToText+eol
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events CheckBox1
