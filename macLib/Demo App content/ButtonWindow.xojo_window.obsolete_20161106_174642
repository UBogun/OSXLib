#tag Events CheckBox2
	#tag Event
		Sub Open()
		  dim apB as new AppleButton(ptr(me.handle))
		  apb.TakeObjectValueFrom new AppleButton(ptr(CheckBox1.Handle))
		  apb.KeyEquivalent = "e"
		  
		End Sub
	#tag EndEvent
#tag EndEvents
